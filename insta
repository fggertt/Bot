def yahoo(email,name):
  global i
  
  
  eml=str(email)
  email=eml.split('@')[0]+'@gmail.com'
  url = 'https://android.clients.google.com/setup/checkavail'
  h = {
    'Content-Length':'98',
    'Content-Type':'text/plain; charset=UTF-8',
    'Host':'android.clients.google.com',
    'Connection':'Keep-Alive',
    'user-agent':'GoogleLoginService/1.3(m0 JSS15J)',
    }
  d = json.dumps({
    'username':email,
    'version':'3',
    'firstName':'AbaLahb',
    'lastName':'AbuJahl'
  })
  res = requests.post(url,data=d,headers=h,proxies=prx)
  if res.json()['status'] == 'SUCCESS':
    print(f'{F} Good Gmail {B} : {F} {email}')
    check(email,user)
  else:
    print(f'{Z} Bad Gmail {B} : {Z} {email}')
